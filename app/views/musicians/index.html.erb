<div>
  <h1>Musicians in Your Scene:</h1>
    <div>
      <% @musicians.each do |musician| %>
        <a href="/musicians/<%= musician.id %>" >
          <div class="card card-1">
            <h4>
            <%= "#{musician.first_name}" + " " + "#{musician.last_name}" %>
            </h4>
          </div>
        </a>
      <% end %>
  </div>
</div>
