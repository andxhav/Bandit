<h1>Create Your Musician Profile!</h1>

<%= form_for [@user, @musician] do |f| %>
  <%= f.label :first_name, "First Name" %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name, "Last Name" %>
  <%= f.text_field :last_name %>

  <%= f.label :home_town, "Home Town" %>
  <%= f.text_field :home_town %>

  <%= f.label :email, "Contact Email" %>
  <%= f.text_field :email %>

  <%= f.label :scene, "Your Scene" %>
  <%= f.text_field :scene %>

  <%= f.label :instruments, "What instruments do you play?" %>
  <%= f.text_field :instruments %>

  <%= f.label :bio, "Bio" %>
  <%= f.text_field :bio %>

  <%= f.submit "Submit Your Profile" %>

<% end %>
